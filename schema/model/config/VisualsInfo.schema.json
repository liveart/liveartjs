{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://liveart/config/VisualsInfo.schema.json",
  "title": "VisualsInfo",
  "description": "Info returned on order.php (when not redirected) or by generateVisuals.php",
  "type": "object",
  "version": "1.0.0",
  "properties": {
    "$schema": {
      "type": "string",
      "description": "Reference to JSON Schema of the supported version"
    },
    "visuals": {
      "type": "object",
      "properties": {
        "zip": {
          "type": "string"
        },
        "previews": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "minLength": 1
              },
              "files": {
                "type": "array",
                "items": {
                  "type": "string",
                  "minLength": 1
                }
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    }
  },
  "required": ["design", "visuals"],
  "additionalProperties": false
}
