{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://liveart/config/vo/Template.schema.json",
  "title": "Template",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "unique": true
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "thumb": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "id",
    "name",
    "thumb"
  ],
  "additionalProperties": false
}
